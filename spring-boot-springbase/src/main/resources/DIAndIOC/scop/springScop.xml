<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans default-lazy-init="false"
       xmlns="http://www.springframework.org/schema/beans"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:p="http://www.springframework.org/schema/p"

       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">

    <bean id="bBean0" scope="prototype" class="com.hzw.learn.springboot.springbase.DIAndIOC.scop.BBean"/>
    <bean id="aBean1" class="com.hzw.learn.springboot.springbase.DIAndIOC.scop.ABean" p:bBean-ref="bBean0"/>
    <bean id="aBean2" class="com.hzw.learn.springboot.springbase.DIAndIOC.scop.ABean" p:bBean-ref="bBean0"/>


    <bean id="bBeanLookup" scope="prototype" class="com.hzw.learn.springboot.springbase.DIAndIOC.scop.BBeanLookup"/>
    <bean id="bBean1" class="com.hzw.learn.springboot.springbase.DIAndIOC.scop.BBean">
        <lookup-method name="createBBean" bean="bBeanLookup" />
    </bean>
    <bean id="aBean3" class="com.hzw.learn.springboot.springbase.DIAndIOC.scop.ABean" p:bBean-ref="bBean1"/>
    <bean id="aBean4" class="com.hzw.learn.springboot.springbase.DIAndIOC.scop.ABean" p:bBean-ref="bBean1"/>

</beans>


